<p>Editer le bar</p>

<%= simple_form_for [:barman, @bar] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.input :description %>
  <%= f.input :average_price %>
  <%= f.input :open_at %>
  <%= f.input :close_at %>
  <%= f.input :photos, as: :file, input_html: { multiple: true }%>

  <%= f.submit %>
<% end %>
